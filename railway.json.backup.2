{
    "build": {
        "builder": "NIXPACKS",
        "buildCommand": "pip install --upgrade pip && pip install PyJWT==2.8.0 && pip install -r requirements.txt"
    },
    "deploy": {
        "startCommand": "uvicorn app.main:app --host 0.0.0.0 --port \",
        "healthcheckPath": "/",
        "healthcheckTimeout": 100,
        "restartPolicyType": "ON_FAILURE"
    }
}
